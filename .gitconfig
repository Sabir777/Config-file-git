	email = sabir.yanin2014@yandex.ru
[core]
	editor = vim
    quotepath = false
[alias]
    alias = !git config --edit --global
    d    = diff
    dc   = diff --cached
	g    = log --all --graph --oneline --decorate --color
    c    = commit
	adc  = !git add . && git commit
    ca   = commit -a
    amend = commit --amend
    s    = status
    rhh  = reset --hard HEAD
    co   = checkout
    com  = checkout master
    cob  = checkout -b
    b    = branch
    bd   = branch -D
    bf   = branch -f
    p    = pull
    ps   = push
    puo  = push -u origin
    pa   = push origin --all
    pf   = push --force-with-lease
    track = checkout --track
